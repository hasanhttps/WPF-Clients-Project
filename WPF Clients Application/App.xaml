<Application x:Class="WPF_Clients_Application.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fa="http://schemas.fontawesome.io/icons/"
             xmlns:local="clr-namespace:WPF_Clients_Application"
             StartupUri="Views/LoginView.xaml"
             Exit="Application_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/MainCustomButtonsRD.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/CustomTextBoxesRD.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/ImagesRD.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/FontsRD.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
